{
  "manifest_version": 3,
  "name": "ARS Google Timerex",
  "version": "1.0",
  "description": "コマンドを入力して特定のページを新規タブで開く拡張機能",
  "permissions": [
    "commands",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "ポップアップを開く"
    }
  }
} 